{
  "name": "@clawdbot/browser-use-kit",
  "version": "1.0.0",
  "description": "Standalone browser control server for Python Agent and other external services",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "browser-server": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "bun dist/cli.js",
    "dev": "bun src/cli.ts",
    "serve": "bun dist/cli.js & bunx http-server public -p 8080 -o"
  },
  "dependencies": {
    "express": "^4.18.2",
    "playwright-core": "^1.40.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}